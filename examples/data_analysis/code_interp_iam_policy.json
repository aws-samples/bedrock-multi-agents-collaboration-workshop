{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ListBucketWithPrefix",
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::*",
            "Condition": {
                "StringLike": {
                    "s3:prefix": [
                        "AGENT_FILE_STORE/*",
                        "CODE_INTERP_FILE_STORE/*"
                    ]
                }
            }
        },
        {
            "Sid": "ReadWriteObjectsWithPrefix",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject"
            ],
            "Resource": [
                "arn:aws:s3:::*/AGENT_FILE_STORE/*",
                "arn:aws:s3:::*/CODE_INTERP_FILE_STORE/*"
            ]
        },
        {
            "Sid": "AmazonBedrockAgentInvokeSubAgentPolicy",
            "Effect": "Allow",
            "Action": "bedrock:InvokeAgent",
            "Resource": [
                "arn:aws:bedrock:*:*:agent/ZNFUYDZIPL",
                "arn:aws:bedrock:*:*:agent-alias/ZNFUYDZIPL/*"
            ]
        }
]
}